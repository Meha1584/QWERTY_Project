<Page x:Class="QWERTY_Project.PageList.checkEqipPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:QWERTY_Project.PageList"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="checkEqipPage">

    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <WrapPanel>
                <Label Content="Название :" FontSize="20"/>
                <Label Name="title" FontSize="20"/>
            </WrapPanel>
            <WrapPanel>
                <Label Content="Количество на складе :" FontSize="20"/>
                <Label Name="count" FontSize="20"/>
            </WrapPanel>
            <WrapPanel>
                <Label Content="Цена :" FontSize="20"/>
                <Label Name="cost" FontSize="20"/>
            </WrapPanel>
            <WrapPanel>
                <Label Content="Значение :" FontSize="20"/>
                <Label Name="value" FontSize="20"/>
            </WrapPanel>
            <WrapPanel>
                <Label Content="Тип :" FontSize="20"/>
                <Label Name="tip" FontSize="20"/>
            </WrapPanel>
        </StackPanel>
    </Grid>
</Page>
